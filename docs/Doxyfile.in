DOXYFILE_ENCODING      = UTF-8
PROJECT_NAME           = "PlockY"
PROJECT_NUMBER         = 1.0
OUTPUT_DIRECTORY       = @CMAKE_CURRENT_BINARY_DIR@/docs
GENERATE_LATEX         = NO
INPUT                  = @CMAKE_CURRENT_SOURCE_DIR@/../include/PlockY @CMAKE_CURRENT_BINARY_DIR@/../src/PlockY
RECURSIVE              = YES
FILE_PATTERNS          = *.h *.hpp *.c *.cpp *.m
EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = YES
EXTRACT_STATIC         = YES
WARN_IF_UNDOCUMENTED   = YES
WARN_NO_PARAMDOC       = YES